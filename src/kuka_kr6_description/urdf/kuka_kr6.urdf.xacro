<?xml version="1.0"?>
<robot xmlns:xacro="http://www.ros.org/wiki/xacro" name="kuka_kr6r900sixx">
  
  <xacro:arg name="use_sim" default="true"/>
  <xacro:arg name="use_fake_hardware" default="false"/>
  <xacro:arg name="sim_gazebo" default="true"/>

  <xacro:property name="mesh_dir" value="file://$(find kuka_kr6_description)"/>

  <material name="kuka_orange">
    <color rgba="0.9647 0.4706 0.1569 1.0"/>
  </material>
  <material name="kuka_grey">
    <color rgba="0.5059 0.4706 0.3882 1.0"/>
  </material>

  <xacro:include filename="$(find kuka_kr6_description)/urdf/kuka_kr6.ros2_control.xacro"/>

  <link name="world"></link>

  <joint name="base joint" type="fixed">
    <origin xyz="0.0 0.0 0.0" rpy="0 0 0"/>
    <parent link="world"/>
    <child link="base_link"/>
  </joint>

  <link name="base_link">
    <visual>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <geometry>
        <mesh filename="${mesh_dir}/meshes/visual/base_link.dae"/>
      </geometry>
      <material name="kuka_grey"/>
    </visual>
    <collision>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <geometry>
        <mesh filename="${mesh_dir}/meshes/collision/base_link.stl"/>
      </geometry>
    </collision>
    <inertial>
      <origin xyz="0 0 0.1" rpy="0 0 0"/>
      <mass value="10.0"/>
      <inertia ixx="0.1" ixy="0" ixz="0" iyy="0.1" iyz="0" izz="0.1"/>
    </inertial>
  </link>

  <link name="link_1">
    <visual>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <geometry>
        <mesh filename="${mesh_dir}/meshes/visual/link_1.dae"/>
      </geometry>
      <material name="kuka_orange"/>
    </visual>
    <collision>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <geometry>
        <mesh filename="${mesh_dir}/meshes/collision/link_1.stl"/>
      </geometry>
    </collision>
    <inertial>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <mass value="5.0"/>
      <inertia ixx="0.05" ixy="0" ixz="0" iyy="0.05" iyz="0" izz="0.05"/>
    </inertial>
  </link>

  <link name="link_2">
    <visual>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <geometry>
        <mesh filename="${mesh_dir}/meshes/visual/link_2.dae"/>
      </geometry>
      <material name="kuka_orange"/>
    </visual>
    <collision>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <geometry>
        <mesh filename="${mesh_dir}/meshes/collision/link_2.stl"/>
      </geometry>
    </collision>
    <inertial>
      <origin xyz="0.2 0 0" rpy="0 0 0"/>
      <mass value="4.0"/>
      <inertia ixx="0.04" ixy="0" ixz="0" iyy="0.04" iyz="0" izz="0.02"/>
    </inertial>
  </link>

  <link name="link_3">
    <visual>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <geometry>
        <mesh filename="${mesh_dir}/meshes/visual/link_3.dae"/>
      </geometry>
      <material name="kuka_orange"/>
    </visual>
    <collision>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <geometry>
        <mesh filename="${mesh_dir}/meshes/collision/link_3.stl"/>
      </geometry>
    </collision>
    <inertial>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <mass value="3.0"/>
      <inertia ixx="0.03" ixy="0" ixz="0" iyy="0.03" iyz="0" izz="0.02"/>
    </inertial>
  </link>

  <link name="link_4">
    <visual>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <geometry>
        <mesh filename="${mesh_dir}/meshes/visual/link_4.dae"/>
      </geometry>
      <material name="kuka_orange"/>
    </visual>
    <collision>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <geometry>
        <mesh filename="${mesh_dir}/meshes/collision/link_4.stl"/>
      </geometry>
    </collision>
    <inertial>
      <origin xyz="0.2 0 0" rpy="0 0 0"/>
      <mass value="2.5"/>
      <inertia ixx="0.02" ixy="0" ixz="0" iyy="0.02" iyz="0" izz="0.01"/>
    </inertial>
  </link>

  <link name="link_5">
    <visual>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <geometry>
        <mesh filename="${mesh_dir}/meshes/visual/link_5.dae"/>
      </geometry>
      <material name="kuka_orange"/>
    </visual>
    <collision>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <geometry>
        <mesh filename="${mesh_dir}/meshes/collision/link_5.stl"/>
      </geometry>
    </collision>
    <inertial>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <mass value="1.5"/>
      <inertia ixx="0.01" ixy="0" ixz="0" iyy="0.01" iyz="0" izz="0.01"/>
    </inertial>
  </link>

  <link name="link_6">
    <visual>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <geometry>
        <mesh filename="${mesh_dir}/meshes/visual/link_6.dae"/>
      </geometry>
      <material name="kuka_grey"/>
    </visual>
    <collision>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <geometry>
        <mesh filename="${mesh_dir}/meshes/collision/link_6.stl"/>
      </geometry>
    </collision>
    <inertial>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <mass value="0.5"/>
      <inertia ixx="0.005" ixy="0" ixz="0" iyy="0.005" iyz="0" izz="0.005"/>
    </inertial>
  </link>

  <link name="flange"/>
  <link name="tool0"/>
  <link name="base"/>

  <joint name="joint_a1" type="revolute">
    <origin rpy="0 0 0" xyz="0 0 0.400"/>
    <parent link="base_link"/>
    <child link="link_1"/>
    <axis xyz="0 0 -1"/>
    <limit effort="150.0" lower="-2.9671" upper="2.9671" velocity="6.2832"/>
    <dynamics damping="10.0" friction="1.0"/>
  </joint>

  <joint name="joint_a2" type="revolute">
    <origin rpy="0 0 0" xyz="0.025 0 0"/>
    <parent link="link_1"/>
    <child link="link_2"/>
    <axis xyz="0 1 0"/>
    <limit effort="150.0" lower="-3.3161" upper="0.7854" velocity="5.2360"/>
    <dynamics damping="10.0" friction="1.0"/>
  </joint>

  <joint name="joint_a3" type="revolute">
    <origin rpy="0 0 0" xyz="0.455 0 0"/>
    <parent link="link_2"/>
    <child link="link_3"/>
    <axis xyz="0 1 0"/>
    <limit effort="100.0" lower="-2.0944" upper="2.7227" velocity="6.2832"/>
    <dynamics damping="10.0" friction="1.0"/>
  </joint>

  <joint name="joint_a4" type="revolute">
    <origin rpy="0 0 0" xyz="0 0 0.035"/>
    <parent link="link_3"/>
    <child link="link_4"/>
    <axis xyz="-1 0 0"/>
    <limit effort="50.0" lower="-3.2289" upper="3.2289" velocity="6.6497"/>
    <dynamics damping="5.0" friction="0.5"/>
  </joint>

  <joint name="joint_a5" type="revolute">
    <origin rpy="0 0 0" xyz="0.420 0 0"/>
    <parent link="link_4"/>
    <child link="link_5"/>
    <axis xyz="0 1 0"/>
    <limit effort="50.0" lower="-2.0944" upper="2.0944" velocity="6.7719"/>
    <dynamics damping="5.0" friction="0.5"/>
  </joint>

  <joint name="joint_a6" type="revolute">
    <origin rpy="0 0 0" xyz="0.080 0 0"/>
    <parent link="link_5"/>
    <child link="link_6"/>
    <axis xyz="-1 0 0"/>
    <limit effort="30.0" lower="-6.1087" upper="6.1087" velocity="10.7338"/>
    <dynamics damping="2.0" friction="0.2"/>
  </joint>

  <joint name="joint_a6-flange" type="fixed">
    <parent link="link_6"/>
    <child link="flange"/>
    <origin rpy="0 0 0" xyz="0 0 0"/>
  </joint>

  <joint name="flange-tool0" type="fixed">
    <parent link="flange"/>
    <child link="tool0"/>
    <origin rpy="0 1.5708 0" xyz="0 0 0"/>
  </joint>

  <xacro:include filename="$(find kuka_kr6_description)/urdf/wsg_gripper.urdf.xacro"/>

 <joint name="gripper-tool0" type="fixed">
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <parent link="tool0"/>
      <child link="wsg_50_base_link"/>
  </joint>

  <joint name="base_link-base" type="fixed">
    <origin rpy="0 0 0" xyz="0 0 0"/>
    <parent link="base_link"/>
    <child link="base"/>
  </joint>
  
  <!--
  <xacro:kuka_kr6_ros2_control
    name="KukaKR6System"
    use_sim="$(arg use_sim)"
    use_fake_hardware="$(arg use_fake_hardware)"
    sim_gazebo="$(arg sim_gazebo)"/>
  -->

  <xacro:if value="$(arg sim_gazebo)">
    <gazebo>
      <plugin filename="gz_ros2_control-system" name="gz_ros2_control::GazeboSimROS2ControlPlugin">
        <parameters>$(find kuka_kr6_description)/config/ros2_controllers.yaml</parameters>
        <robot_param>robot_description</robot_param>
        <robot_param_node>robot_state_publisher</robot_param_node>
      </plugin>
    </gazebo>
  </xacro:if>

</robot>
